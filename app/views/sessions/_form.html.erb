<%= form_with url: sessions_path do |form| %>

    <div>
        <%= form.label :email %></br>
        <%= form.email_field :email%>
    </div>
  
    <div>
        <%= form.label :password %></br>
        <%= form.password_field :password%>
    </div>
  
    <div>
        <%= form.submit "Log in"%>
    </div>

<% end %>
  
<%= link_to "Sign up", new_user_path %>